<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="150.0" prefWidth="320" style="-fx-background-color: #006099;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.zero2infinity.search.snow.FXMLController">
    <children>
      <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0">
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <HBox spacing="10.0">
                     <children>
                          
                          <Label style="-fx-text-fill: #ffffff; -fx-font-weight: bold;" text="SNOW " textAlignment="CENTER" />
                        <Label style="-fx-text-fill: #ffffff;" text="ctrl + alt + shift + s" textAlignment="CENTER">
                           <font>
                              <Font size="11.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <HBox spacing="10.0">
                     <children>
                          <TextField fx:id="txtfld_appname" promptText="Enter Application Name" />
                          <Button fx:id="btn_search" mnemonicParsing="false" onAction="#handleButtonAction" style="-fx-background-color: #8eb8d2; -fx-text-fill: #ffffff; -fx-font-weight: bold;" text="Search" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <HBox spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                          <RadioButton fx:id="rbtnoffshore" mnemonicParsing="false" style="-fx-text-fill: #ffffff;" text="Offshore" />
                         
                          <RadioButton fx:id="rbtnonshore" mnemonicParsing="false" style="-fx-text-fill: #ffffff;" text="Onshore" />
                        <CheckBox fx:id="cbclipboard" mnemonicParsing="false" style="-fx-text-fill: #ffffff;" text="Clipboard" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                    <Label fx:id="label_errorbox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Log" textAlignment="CENTER" textFill="#7ac4ee" VBox.vgrow="ALWAYS">
                        <font>
                            <Font size="11.0" />
                        </font>
                    </Label>
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <HBox spacing="10.0">
                     <children>
                          <TextField fx:id="txtfld_filepath" disable="true" promptText="Excel File path" />
                          <Button fx:id="btn_filepicker" mnemonicParsing="false" onAction="#handleButtonAction" style="-fx-background-color: #8eb8d2; -fx-text-fill: #ffffff;" text="..." />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
    </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</AnchorPane>
